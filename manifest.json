{
    "name": "SOS Dev Tips",
    "description": "L'extension qui vous aide à trouver des solutions à vos problèmes de développement web !",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
      "notifications",
      "storage"
    ],
    "background": {
      "service_worker": "services.js"
    },
    "action": {
      "default_title": "SOS Dev Tips",
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "16.png",
      "32": "32.png",
      "48": "48.png",
      "128": "128.png"
    }
  }