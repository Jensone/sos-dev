{
  "name": "SOS DevTips",
  "description": "L'extension qui vous aide à trouver des solutions pour vos problèmes en développement web !",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "notifications",
    "storage",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "SOS DevTips",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "/assets/logo/16.png",
    "32": "/assets/logo/32.png",
    "48": "/assets/logo/48.png",
    "128": "/assets/logo/128.png"
  }
}